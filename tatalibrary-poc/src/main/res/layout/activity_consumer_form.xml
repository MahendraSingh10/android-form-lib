<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ConsumerFormActivity">


    <EditText
        android:id="@+id/editTextText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#A89494"
        android:ems="10"
        android:inputType="text"
        android:text="Name"
        tools:layout_editor_absoluteX="195dp"
        tools:layout_editor_absoluteY="331dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="132dp"
        android:text="User Name"
        app:layout_constraintEnd_toStartOf="@+id/editTextText"
        tools:layout_editor_absoluteY="331dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        tools:layout_editor_absoluteX="9dp"
        tools:layout_editor_absoluteY="381dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#7A4C4C"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="400dp" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#710909"
        android:ems="10"
        android:inputType="phone"
        tools:layout_editor_absoluteX="185dp"
        tools:layout_editor_absoluteY="470dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/cardview_shadow_start_color"
        android:text="Phone"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="470dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="239dp"
        android:layout_height="66dp"
        android:background="#6A1717"
        android:text="Save Information"
        android:textColor="#DDDADA"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="537dp" />

    <FrameLayout
        android:layout_width="409dp"
        android:layout_height="329dp"
        android:background="#936464"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="86dp">

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>